{
  "name": "x-wall-street",
  "displayName": "X Wall Street BETA",
  "version": "0.0.2",
  "description": "THIS EXTENSION IS FOR BETA TESTING",
  "author": "flash pig",
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "build:firefox": "tsc --noEmit && cross-env __FIREFOX__=true vite build",
    "build:watch": "cross-env __DEV__=true vite build -w --mode development",
    "build:firefox:watch": "cross-env __DEV__=true __FIREFOX__=true vite build -w --mode development",
    "build:hmr": "rollup --config utils/reload/rollup.config.mjs",
    "wss": "node utils/reload/initReloadServer.js",
    "dev": "pnpm build:hmr && (run-p wss build:watch)",
    "dev:firefox": "pnpm build:hmr && (run-p wss build:firefox:watch)",
    "test": "vitest",
    "commitlint": "commitlint --edit",
    "lint": "eslint src --ext .ts,.js,.tsx,.jsx",
    "lint:fix": "pnpm lint --fix",
    "prettier": "prettier . --write",
    "prepare": "husky install"
  },
  "type": "module",
  "dependencies": {
    "@ant-design/cssinjs": "^1.18.2",
    "@ant-design/icons": "^5.2.6",
    "@plasmohq/storage": "^1.9.0",
    "@walletconnect/sign-client": "^2.11.0",
    "@web3auth/base": "^7.2.1",
    "@web3auth/ethereum-provider": "^7.2.1",
    "@web3auth/modal": "^7.3.0",
    "@web3auth/no-modal": "^7.2.1",
    "@web3auth/openlogin-adapter": "^7.2.1",
    "antd": "5.12.8",
    "construct-style-sheets-polyfill": "3.1.0",
    "jwt-decode": "^4.0.0",
    "phaser": "^3.70.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "web3": "4.2.2",
    "webextension-polyfill": "0.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "18.4.4",
    "@commitlint/config-conventional": "18.1.0",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@rollup/plugin-typescript": "11.1.5",
    "@testing-library/react": "14.0.0",
    "@types/chrome": "0.0.251",
    "@types/node": "18.19.0",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.18",
    "@types/ws": "8.5.8",
    "@typescript-eslint/eslint-plugin": "6.10.0",
    "@typescript-eslint/parser": "6.18.1",
    "@vitejs/plugin-react": "4.2.0",
    "chokidar": "3.5.3",
    "cross-env": "7.0.3",
    "eslint": "8.56.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "fs-extra": "11.1.1",
    "husky": "8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "15.2.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.1.0",
    "rollup": "4.3.0",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "sass": "1.69.5",
    "ts-loader": "9.5.0",
    "tslib": "2.6.2",
    "typescript": "5.2.2",
    "vite": "5.0.11",
    "vitest": "^0.34.6",
    "ws": "8.14.2"
  },
  "engines": {
    "node": ">=18.x",
    "npm": ">=10.x"
  }
}